CREATE EXTERNAL TABLE IF NOT EXISTS policepractice.yelp (
  `id` string,
  `alias` string,
  `image_url` string,
  `url` string,
  `review_count` int,
  `rating` double,
  `transactions` string,
  `price` string,
  `phone` string,
  `display_phone` string,
  `distance` double 
)
ROW FORMAT  serde 'org.apache.hive.hcatalog.data.JsonSerDe'
WITH SERDEPROPERTIES (
  'serialization.format' = '1'
  'ignore.malformed.json' = 'true'
) LOCATION 's3://police-store-again/JSON/'
TBLPROPERTIES ('has_encrypted_data'='false');

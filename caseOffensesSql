CREATE EXTERNAL TABLE IF NOT EXISTS policepractice.caseoffenses (
  `caseid` int,
  `casenumber` int,
  `reportdate` date,
  `reporttime` timestamp,
  `dow` string,
  `occurredincident` string,
  `statute` string,
  `ac` string,
  `crimeclass` string,
  `ucroffense` string,
  `ucrstolenproperty` string,
  `addressid` int,
  `housenumber` int,
  `streetprefix` string,
  `streetname` string,
  `streettype` string,
  `apt` string,
  `xtreetprefix` string,
  `xstreetname` string,
  `xstreettype` string,
  `city` string,
  `state` string,
  `zip` int,
  `beat` int,
  `shieldnumber` int,
  `officerfirstname` string,
  `officerlastname` string,
  `status` string,
  `dispo` string,
  `domesticcode` string,
  `location` string,
  `methodofentry` string,
  `methodofexit` string,
  `pointofentry` string,
  `pointofexit` string,
  `security` string,
  `tool` string,
  `mo` string 
)
ROW FORMAT SERDE 'org.apache.hadoop.hive.serde2.lazy.LazySimpleSerDe'
WITH SERDEPROPERTIES (
  'serialization.format' = ',',
  'field.delim' = ','
) LOCATION 's3://police-store-again/CSV-Files/'
TBLPROPERTIES ('has_encrypted_data'='false');
